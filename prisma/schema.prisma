// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./local.db"
}

model Troops {
  id        Int      @id @default(autoincrement())
  nama      String
  kekuatan  Int
  desc      String
  createdAt DateTime @default(now())
  
  @@map("troops")
}